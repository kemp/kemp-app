    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "autosize": "^4.0.2",
    "cross-env": "^5.2.0",
    "glob-all": "^3.1.0",
    "laravel-mix": "^4.0.14",
    "nprogress": "^0.2.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "resolve-url-loader": "2.3.1",
    "sass": "^1.17.2",
    "sass-loader": "7.*",
    "tailwindcss": "^0.7.4",
    "vue": "^2.6.8",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.8"
  }
